<?xml version="1.0" encoding="utf-8"?>

<package schemaVersion="1.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="PACK.xsd">
  <vendor>STMicroelectronics</vendor>
  <name>X-CUBE-MEMS1</name>
  <description>Drivers and sample applications for MEMS components</description>
  <license>en.DM00367782.pdf</license>
  <!-- web download link -->
  <url>http://sw-center.st.com/packs/x-cube/</url>

  <releases>
    <release version="9.2.0" date="2022-02-09">
      ToDo: Change date before release and delete this line
      Add new sensors: LIS2DU12, ILPS22QS, ILPS28QSW, LPS28DFW
      Add support for Nucleo-U5
      Minor improvement in DataLogExtended
      NOTE: on dual-core STM32 series this expansion software can be used on both cores but exclusively
    </release>
    <release version="9.1.0" date="2021-10-05">
      Recompile motion libraries with FPU Single Precision for Cortex-M7
      Add new sensors: LIS2DTW12, LPS22DF
      NOTE: on dual-core STM32 series this expansion software can be used on both cores but exclusively
    </release>
    <release version="9.0.0" date="2021-06-15">
      Remove bundles from Middlewares and BSP custom pack components to align with Keil pack specifications
      Add motion libraries for Cortex-M33
      Add new sensors: AIS2IH, LSM6DSO32X
      Add support to Example Selector
      NOTE: on dual-core STM32 series this expansion software can be used on both cores but exclusively
    </release>
    <release version="8.3.0" date="2021-03-15">
      Update AccelCalibration2_IIS2ICLX, DataLogFusion, ECompass, GyroscopeCalibration, IntensityDetection, MagnetometerCalibration, TiltSensing, TiltSensing2_IIS2ICLX middleware applications
      Update drivers
      NOTE: on dual-core STM32 series this expansion software can be used on both cores but exclusively
    </release>
    <release version="8.2.0" date="2020-11-13">
      Add ActiveTime, ActivityRecognition, ActivityRecognitionWrist, FallDetection, FitnessActivities, Pedometer, PedometerWrist, PoseEstimation, StandingSittingDesk middleware applications
      Add support for LPS22CH and LPS27HHTW
      NOTE: on dual-core STM32 series this expansion software can be used on both cores but exclusively
    </release>
    <release version="8.1.1" date="2020-09-12">
      Change Virtual Signals according to new rules
    </release>
    <release version="8.1.0" date="2020-08-31">
      Add AccelCalibration2_IIS2ICLX, TiltSensing2_IIS2ICLX middleware applications
    </release>
    <release version="8.0.0" date="2020-06-19">
      Create sample applications with STM32CubeMX V6.0.0 and upwards
      Update metadata names
      Add support for LPS33K and IIS2ICLX
      Add support for IKS02A1 in DataLogFusion, GyroscopeCalibration, AccelerometerCalibration, MagnetometerCalibration, TiltSensing, ECompass, DynamicInclinometer applications
      Integrate all middlewares applications into 3 common files
    </release>
    <release version="7.2.0" date="2020-05-07">
      Add support to DynamicInclinometer, AirplaneDetection
      Add support to LSM6DSO32
    </release>
    <release version="7.1.0" date="2020-01-31">
      Add support to IntensityDetection, DataLogFusion, GyroscopeCalibration, AccelerometerCalibration, MagnetometerCalibration, TiltSensing, VerticalContext, CarryPosition, ECompass, GestureRecognition
    </release>
    <release version="7.0.0" date="2019-10-07">
      Add support to X-NUCLEO-IKS02A1
      Add support to AIS2DW12, LSM6DSR, STTS22H, A3G4250D, AIS328DQ, AIS3624DQ, H3LIS331DL, LSM6DSRX, ISM330DHCX
      Create sample applications with STM32CubeMX V5.3.0 and upwards
      Update PDSC to be compliant with XSD v1.6.0
    </release>
    <release version="6.2.0">
      Create sample applications with STM32CubeMX V5.2.0 and upwards
      This release is not fully compatible with STM32CubeMX V4.27.0, STM32CubeMX V5.0.0, STM32CubeMX V5.0.1 and STM32CubeMX V5.1.0
      Add support to ASM330LHH, IIS2DLPC, IIS2MDC, ISM303DAC, ISM330DLC, LIS2DH12, LPS33HW, LSM6DSOX
    </release>
    <release version="6.1.0">
      Create sample applications with STM32CubeMX V5.2.0 and upwards
      This release is not fully compatible with STM32CubeMX V4.27.0, STM32CubeMX V5.0.0, STM32CubeMX V5.0.1 and STM32CubeMX V5.1.0
    </release>
    <release version="6.0.1">
      Create sample applications with STM32CubeMX V5.1.0 and upwards
      This release is not fully compatible with STM32CubeMX V4.27.0, STM32CubeMX V5.0.0 and STM32CubeMX V5.0.1
      Fix issue on STTS751_TemperatureLimit example for F4 and L4
    </release>
    <release version="6.0.0">
      Create sample applications with STM32CubeMX V5.1.0 and upwards
      This release is not fully compatible with STM32CubeMX V4.27.0, STM32CubeMX V5.0.0 and STM32CubeMX V5.0.1
      Add support to STM32G0 family
      Add support to X-NUCLEO-IKS01A3
    </release>
    <release version="5.2.1">
      Create sample applications with STM32CubeMX V5.0.0 and STM32CubeMX V5.0.1
      This release is not fully compatible with STM32CubeMX V4.27.0
      STM32G0 family not supported yet
      Fix issue in the rounding of temperature values in DataLogTerminal and IKS01A2_DataLogTerminal applications
    </release>
    <release version="5.2.0">
      Create sample applications with STM32CubeMX V5.0.0 and upwards
      This release is not fully compatible with STM32CubeMX V4.27.0
    </release>
    <release version="5.1.0">
      First official release of the MEMS support package
      Create sample applications with STM32CubeMX V4.27.0
      This release is fully compatible only with STM32CubeMX V4.27.0
    </release>
  </releases>

  <taxonomy>
    <description  Cclass="Board Part">The MEMS components support class</description>
    <description  Cclass="Board Extension">The expansion boards support class</description>
  </taxonomy>

  <keywords>
    <keyword>ARM</keyword>
    <keyword>Cortex-M</keyword>
    <keyword>STMicroelectronics</keyword>
    <keyword>MEMS</keyword>
    <keyword>IMPLEMENTED_WITH_STM32CUBEMX</keyword>
    <keyword>CUBEMX_MIGRATION_CAPABILITY</keyword>
    <keyword>IS_CONTEXT_EXCLUSIVE</keyword> <!-- Do not allow to instantiate the pack in more than one context -->
    <keyword>DNS_BUSIO_X_CONTEXT_SHARING</keyword> <!-- Do not allow to share resources across context -->
    <keyword>DOES_NOT_SUPPORT_SECURE</keyword> <!-- Do not support secure context -->
  </keywords>

  <apis>
    <!-- API -->
    <api Cclass="Board Extension" Cgroup="IKS01A3" Capiversion="2.6.4" exclusive="1">
      <description>MEMS library for X-NUCLEO-IKS01A3</description>
      <files>
        <file category="doc"    name="Documentation/X_CUBE_MEMS1.chm"/>
      </files>
    </api>
    <api Cclass="Board Extension" Cgroup="IKS01A2" Capiversion="2.6.4" exclusive="1">
      <description>MEMS library for X-NUCLEO-IKS01A2</description>
      <files>
        <file category="doc"    name="Documentation/X_CUBE_MEMS1.chm"/>
      </files>
    </api>
    <api Cclass="Board Extension" Cgroup="IKS02A1" Capiversion="2.6.4" exclusive="1">
      <description>MEMS library for X-NUCLEO-IKS02A1</description>
      <files>
        <file category="doc"    name="Documentation/X_CUBE_MEMS1.chm"/>
      </files>
    </api>
  </apis>

  <conditions>
    <condition id="Cortex-M Device">
      <description>Cortex-M processor based device: Cortex-M0, Cortex-M0+, Cortex-M3, Cortex-M4, Cortex-M7, Cortex-M33</description>
      <!-- STM32F0 -->
      <accept Dcore="Cortex-M0"/>

      <!-- STM32L0 and STM32G0 -->
      <accept Dcore="Cortex-M0+"/>

      <!-- STM32F1, STM32F2 and STM32L1 -->
      <accept Dcore="Cortex-M3"/>

      <!-- STM32F4, STM32F3 and STM32L4 series -->
      <accept Dcore="Cortex-M4"/>

      <!-- STM32F7 and STM32H7 series -->
      <accept Dcore="Cortex-M7"/>

      <!-- STM32L5 series -->
      <accept Dcore="Cortex-M33"/>
    </condition>

    <condition id="Cortex-M Device for MW_M0_M3_M4_M7_M33">
      <description>Middlewares support for Cortex-M processor based device: Cortex-M0, Cortex-M0+, Cortex-M3, Cortex-M4, Cortex-M7, Cortex-M33</description>
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <accept Dcore="Cortex-M3"/>
      <accept Dcore="Cortex-M4"/>
      <accept Dcore="Cortex-M7"/>
      <accept Dcore="Cortex-M33"/>
    </condition>

    <condition id="Cortex-M Device for MW_M0_M3_M4_M7">
      <description>Middlewares support for Cortex-M processor based device: Cortex-M0, Cortex-M0+, Cortex-M3, Cortex-M4, Cortex-M7</description>
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <accept Dcore="Cortex-M3"/>
      <accept Dcore="Cortex-M4"/>
      <accept Dcore="Cortex-M7"/>
      <deny Dcore="Cortex-M33"/>
    </condition>

    <condition id="Cortex-M Device for MW_M3_M4_M7_M33">
      <description>Middlewares support for Cortex-M processor based device: Cortex-M3, Cortex-M4, Cortex-M7, Cortex-M33</description>
      <accept Dcore="Cortex-M3"/>
      <accept Dcore="Cortex-M4"/>
      <accept Dcore="Cortex-M7"/>
      <accept Dcore="Cortex-M33"/>
      <deny Dcore="Cortex-M0"/>
      <deny Dcore="Cortex-M0+"/>
    </condition>

    <condition id="Cortex-M Device for MW_M3_M4_M7">
      <description>Middlewares support for Cortex-M processor based device: Cortex-M3, Cortex-M4, Cortex-M7</description>
      <accept Dcore="Cortex-M3"/>
      <accept Dcore="Cortex-M4"/>
      <accept Dcore="Cortex-M7"/>
      <deny Dcore="Cortex-M0"/>
      <deny Dcore="Cortex-M0+"/>
      <deny Dcore="Cortex-M33"/>
    </condition>

    <condition id="CM0_GCC">
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="CM0_IAR">
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="CM0_Keil">
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="CM3_GCC">
      <require Dcore="Cortex-M3"/>
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="CM3_IAR">
      <require Dcore="Cortex-M3"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="CM3_Keil">
      <require Dcore="Cortex-M3"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="CM4_GCC">
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="GCC"/>
      <deny Dfamily="STM32WL"/>
    </condition>
    <condition id="CM4_IAR">
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="IAR"/>
      <deny Dfamily="STM32WL"/>
    </condition>
    <condition id="CM4_Keil">
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="ARMCC"/>
      <deny Dfamily="STM32WL"/>
    </condition>

    <condition id="CM4_GCC_NoFPU">
      <require Dcore="Cortex-M4" Dfamily="STM32WL"/>
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="CM4_IAR_NoFPU">
      <require Dcore="Cortex-M4" Dfamily="STM32WL"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="CM4_Keil_NoFPU">
      <require Dcore="Cortex-M4" Dfamily="STM32WL"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="CM7_GCC">
      <require Dcore="Cortex-M7"/>
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="CM7_IAR">
      <require Dcore="Cortex-M7"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="CM7_Keil">
      <require Dcore="Cortex-M7"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="CM33_GCC">
      <require Dcore="Cortex-M33"/>
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="CM33_IAR">
      <require Dcore="Cortex-M33"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="CM33_Keil">
      <require Dcore="Cortex-M33"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="ACCGYR_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="AccGyr"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="MOTION_SENSOR"/>
    </condition>
    <condition id="ACCMAG_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="AccMag"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="MOTION_SENSOR"/>
    </condition>
    <condition id="ACCTEMP_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="AccTemp"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="HYBRID_SENSOR"/>
    </condition>
    <condition id="ACC_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="Acc"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="MOTION_SENSOR"/>
    </condition>
    <condition id="GYR_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="Gyr"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="MOTION_SENSOR"/>
    </condition>
    <condition id="MAG_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="Mag"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="MOTION_SENSOR"/>
    </condition>
    <condition id="PRESSTEMP_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="PressTemp"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="ENV_SENSOR"/>
    </condition>
    <condition id="PRESSTEMPQVAR_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="PressTempQvar"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="ENV_SENSOR"/>
    </condition>
    <condition id="HUMTEMP_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="HumTemp"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="ENV_SENSOR"/>
    </condition>
    <condition id="TEMP_WITH_BSP_CUSTOM">
      <require Cclass="Board Part" Cgroup="Temp"/>
      <require Cclass="Board Support" Cgroup="Custom" Csub="ENV_SENSOR"/>
    </condition>
    <condition id="ACCELEROMETER_SENSOR_WITH_BSP_CUSTOM">
      <accept condition="ACCGYR_WITH_BSP_CUSTOM"/>
      <accept condition="ACCMAG_WITH_BSP_CUSTOM"/>
      <accept condition="ACCTEMP_WITH_BSP_CUSTOM"/>
      <accept condition="ACC_WITH_BSP_CUSTOM"/>
    </condition>
    <condition id="GYROSCOPE_SENSOR_WITH_BSP_CUSTOM">
      <accept condition="ACCGYR_WITH_BSP_CUSTOM"/>
      <accept condition="GYR_WITH_BSP_CUSTOM"/>
    </condition>
    <condition id="MAGNETIC_SENSOR_WITH_BSP_CUSTOM">
      <accept condition="ACCMAG_WITH_BSP_CUSTOM"/>
      <accept condition="MAG_WITH_BSP_CUSTOM"/>
    </condition>
    <condition id="PRESSURE_SENSOR_WITH_BSP_CUSTOM">
      <accept condition="PRESSTEMP_WITH_BSP_CUSTOM"/>
      <accept condition="PRESSTEMPQVAR_WITH_BSP_CUSTOM"/>
    </condition>
    <condition id="TEMPERATURE_SENSOR_WITH_BSP_CUSTOM">
      <accept condition="ACCTEMP_WITH_BSP_CUSTOM"/>
      <accept condition="HUMTEMP_WITH_BSP_CUSTOM"/>
      <accept condition="PRESSTEMP_WITH_BSP_CUSTOM"/>
      <accept condition="PRESSTEMPQVAR_WITH_BSP_CUSTOM"/>
      <accept condition="TEMP_WITH_BSP_CUSTOM"/>
    </condition>
    <condition id="HUMIDITY_SENSOR_WITH_BSP_CUSTOM">
      <accept condition="HUMTEMP_WITH_BSP_CUSTOM"/>
    </condition>

